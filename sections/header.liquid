<style>
.item_badge__count {
position: absolute;
right: -5px;
top: 0px;
background: #e4c13c;
border-radius: 50px;
height: 20px;
width: 20px;
color: black;
text-align: center;
display: flex;
vertical-align: middle;
align-items: center;
justify-content: center;
font-size: 10px;
font-weight: 500;
}
  .social-icons__link-header > svg {
    color: #e5c966;
}
  svg.Icon.Icon--cart.icon {
    width: 38px;
    height: 38px;
}
  .gm-menu:not(.gm-menu-mobile) .aMCIV{display:none!immportant;}

  @media only screen and (min-width:967px) {
   .social-icons__item:last-child {
    margin-left: 20px;
     position: relative;
}
}
.mobile-nav-wrapper {
    overflow: scroll;
    height: 100vh !important;
}
